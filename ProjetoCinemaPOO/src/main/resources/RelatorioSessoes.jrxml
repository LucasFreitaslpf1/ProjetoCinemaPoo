	<queryString language="SQL">
		<![CDATA[SELECT projetocinema.sessao.horario,
	projetocinema.sessao.audio,
	projetocinema.sessao.valorsessao,
	projetocinema.filme.nome,
	projetocinema.filme.classificacao,
	projetocinema.sala.numero,
	projetocinema.sala.tela
